{
    "compilerOptions": {
        "target": "es2018",
        "module": "commonjs",
        "sourceMap": false,
        "rootDir": "src/",
        "outDir": "scripts/",
        "strict": true,
        "strictPropertyInitialization": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true
    }
}
